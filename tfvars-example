# IBM Cloud Region for VPC
region = ""

# This will be used to prefix all the resources created.
# If not set, a random 4 character string will be used.
project_prefix = ""

# Name of an existing Resource Group.
# If not set, a new one will be created
existing_resource_group = ""

# Name of an existing SSH key in the Region.
# If not set, a new one will be created
existing_ssh_key = ""

# Name of an existing COS instance.
# If not set, a new one will be created
existing_cos_instance = ""
